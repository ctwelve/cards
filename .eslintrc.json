{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:react/jsx-runtime"
	],
	"overrides": [],
	"settings": { "react": { "version": "detect" } },
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": ["react"],
	"rules": {
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"semi": ["error", "always"],
		"no-console": "off",
		"no-unused-vars": "warn"
	},
	"globals": { "console": false }
}
